<h1>Upload a post</h1>
<div>
<%= form_for :post, url: user_posts_path(@user), method: :post do |f| %>
    <ul>
      <li><%= f.label :title %><li>
      <li><%= f.text_field :title %></li>
    </ul>
     <ul>
        <li><%= f.label :content %></li>
        <li><%= f.text_field :content %></li>
    </ul>
    <div class="pushDown30">
      <%= f.submit %>
    </div>
  </div>
<% end %>
